# Environment variables for local development
# Copy to .env and fill in values

# RapidAPI key for property lookups (us-real-estate-listings)
RAPIDAPI_KEY=

# Auth — admin email (only this email can log in)
HF_ADMIN_EMAIL=

# Auth — SMTP settings for sending magic link emails
HF_SMTP_HOST=
HF_SMTP_PORT=587
HF_SMTP_USER=
HF_SMTP_PASS=
HF_SMTP_FROM=

# Auth — dev mode (logs magic links to console instead of sending email)
HF_DEV_MODE=true

# Server base URL (used in magic link emails)
HF_BASE_URL=http://localhost:8080
